{
  "name": "githubsearch",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@apollo/client": "^3.3.21",
    "@lottiefiles/react-lottie-player": "^3.3.1",
    "@material-ui/core": "^4.12.1",
    "@material-ui/lab": "^4.0.0-alpha.60",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^26.0.15",
    "@types/node": "^12.0.0",
    "@types/react": "^17.0.0",
    "@types/react-dom": "^17.0.0",
    "graphql": "^15.5.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "styled-components": "^5.3.0",
    "typescript": "^4.1.2",
    "web-vitals": "^1.0.1"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.9.6",
    "@cypress/instrument-cra": "^1.4.0",
    "@cypress/react": "^4.16.3",
    "@graphql-codegen/cli": "1.21.7",
    "@graphql-codegen/introspection": "1.18.2",
    "@graphql-codegen/typescript": "1.23.0",
    "@graphql-codegen/typescript-operations": "1.18.4",
    "@graphql-codegen/typescript-react-apollo": "2.3.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.1",
    "@testing-library/cypress": "^7.0.6",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/cypress-cucumber-preprocessor": "^4.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.8",
    "@types/react-dom": "^17.0.5",
    "@types/react-router-dom": "^5.1.7",
    "@types/react-table": "^7.7.1",
    "@types/sanitize-html": "^2.3.1",
    "@types/styled-components": "^5.1.9",
    "@typescript-eslint/parser": "^4.25.0",
    "cypress": "^7.7.0",
    "cypress-cucumber-preprocessor": "^4.0.0",
    "eslint": "^7.30.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^7.1.0",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-react": "^7.21.5",
    "eslint-plugin-react-hooks": "^4.0.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "lint-staged": "^11.0.0",
    "lodash.sortby": "^4.7.0",
    "react-scripts": "4.0.3",
    "typescript": "^4.3.2"
  },
  "scripts": {
    "start": "react-scripts -r @cypress/instrument-cra start",
    "build": "react-scripts build",
    "lint": "eslint .  --ext .js,.jsx,.tsx",
    "lint:fix": "eslint .  --ext .js,.jsx,.tsx --fix",
    "test": "react-scripts test --passWithNoTests",
    "test:e2e": "npx cypress open",
    "test:e2e:integration": "npx cypress run -e TAGS='not @smoke'",
    "test:e2e:smoke": "npx cypress run -e TAGS='@smoke'",
    "eject": "react-scripts eject",
    "codegen": "graphql-codegen --config codegen.yml",
    "deploy": "docker build ."
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "cypress-cucumber-preprocessor": {
    "commonPath": "cypress/integration/common/",
    "nonGlobalStepDefinitions": true
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "lint-staged && yarn test"
    }
  },
  "lint-staged": {
    "app/**/*.{js,jsx,ts,tsx,json,css}": [
      "yarn lint:fix"
    ]
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "all": true
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
